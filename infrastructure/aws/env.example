# Database Configuration
POSTGRES_USER=mysic_user
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=mysic_db

# Database URL (Docker Compose 환경 변수 확장 문제 해결을 위해 직접 설정 권장)
# ⚠️ 비밀번호를 실제 비밀번호로 변경해야 합니다
DATABASE_URL=postgresql://mysic_user:your-secure-password-here@postgres:5432/mysic_db

# Backend Configuration
SECRET_KEY=your-secret-key-change-this-in-production-min-32-chars
ENVIRONMENT=production
CORS_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8000

# AWS Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET=your-s3-bucket-name

# Database URL (if using external database like RDS)
# DATABASE_URL=postgresql://user:password@rds-endpoint:5432/dbname

